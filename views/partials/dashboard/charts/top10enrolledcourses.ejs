<div class="col-md-6 col-xxl-6 d-flex order-2 order-xxl-6">
  <div class="card flex-fill w-100">
    <div class="px-4 pt-3">
      <h5 class="card-title mb-0">Top 10 Enrolled Courses</h5>
    </div>
    <div class="px-4 pb-4 d-flex">
      <div class="align-self-center w-100">
        <div class="py-3">
          <div class="chart chart-xs">
            <canvas id="TopcoursesBarchart"></canvas>
          </div>
        </div>
      </div>
    </div>
  </div>
