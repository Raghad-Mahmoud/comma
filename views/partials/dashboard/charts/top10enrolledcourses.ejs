<script>
  const Top10Enrolledcourses0 = <%-JSON.stringify(analytics.Top10Enrolledcourses)%>;
  const name = Top10Enrolledcourses0.map((item) => item.title.slice(0, 20)); // edit 
  const enrolledUsers = Top10Enrolledcourses0.map((item) => item.enrolledUsers);
  const tBr = document.getElementById('TopcoursesBarchart')
  const data = {
    labels: name,
    datasets: [{
      label: 'Top 10 EnrolledCourses ',
      data: enrolledUsers,
    }]
  }
  const myChart = new Chart(tBr, {
    type: 'bar',
    data: data,
  })
</script>